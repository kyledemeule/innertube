{
  "manifest_version": 2,

  "name": "Innertube",
  "description": "Seach Youtube videos",
  "version": "1.0",

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://cdnjs.cloudflare.com https://apis.google.com; object-src 'self'",

  "browser_action": {
   "default_icon": "static/base.png",
   "default_popup": "views/popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "identity"
  ],
  "content_scripts": [
     {
       "matches": ["https://www.youtube.com/*"],
       "js": ["scripts/content.js"]
     }
   ],
   "oauth2": {
    "client_id": "36897722787-f93quu7b90iparjl9nc6i3q1asem7qui.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/youtube.force-ssl"]
  },
  "key": ""
}
